<metal:nav_list define-macro="nav_list">
<nav class="pure-menu pure-menu-open">
<ul>
<li class="pure-menu-heading" ><a href="/">JASMIN</a></li>
<div tal:omit-tag="" tal:repeat="(itemId, item) nav.items()">
<li
tal:repeat="aspect item.get('_links', [])"
tal:attributes="
class 'pure-menu-selected' if aspect.rel == 'self' else ''
">
<a 
tal:condition="aspect.rel == 'canonical'"
tal:content="aspect.name"
tal:attributes="
rel aspect.rel;
href aspect.typ.format(aspect.ref);
"></a>
<a 
tal:condition="aspect.rel == 'self'"
tal:content="aspect.name"
tal:attributes="
rel aspect.rel;
href '#';
"></a>
</li>
</div>
</ul>
</nav>
</metal:nav_list>
