<html metal:use-macro="load: base.pt" lang="en">
<nav metal:fill-slot="nav" class="pure-menu pure-menu-open">
    <ul>
    <li class="pure-menu-heading" ><a href="/">JASMIN</a></li>
    <li><a href="#">Account</a></li>
    <li><a href="/#">JVO</a></li>
    <li><a href="/hosts">VMs</a></li>
    <li><a href="/people">People</a></li>
    </ul>
</nav>
<div metal:fill-slot="summary">
<section id="items">
<h1>Membership</h1>
<dl>
<div tal:omit-tag=""
tal:repeat="(itemId, item) items.items()"
tal:attributes="
id itemId;
">
<dt tal:content="item['data']['type']"></dt>
<dd tal:attributes="class item['data']['type'].lower();">${item.value}</dd>
</div>
</dl>
</section>
</div>
<section metal:fill-slot="detail" id="options">
<dl tal:repeat="option options.values()"
tal:attributes="
class python:' '.join('{0.fsm} {0.name}'.format(s) for s in option['states']);">
<div tal:omit-tag="" tal:repeat="(attr, val) sorted(option['data'].items())">
<dt>${attr}</dt><dd>${val}</dd>
</div>
</dl>
</section>
</html>
